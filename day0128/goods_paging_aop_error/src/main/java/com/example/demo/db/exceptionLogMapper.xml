<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="exceptionlog">
  <insert id="insert" parameterType="exceptionLog">
  	insert into exceptionlog values(seq_exceptionlog.nextval,#{msg},#{uri},sysdate)
  </insert>
</mapper>